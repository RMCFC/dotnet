
@{ 
    ViewData["Title"] = "Employee Page";
}

<div class="text-center">
   <h1> Employee</h1>

   @{   @using DAL;
        List<Employee> emp1=(List<Employee>)ViewData["catalog"]; 
        <table><tr>
            @foreach(Employee e in emp1){
                <td>@e.Id</td>
                <td>@e.Fname</td>
                <td>@e.Lname</td>

            }
            </tr>
        </table>
    } 


</div>